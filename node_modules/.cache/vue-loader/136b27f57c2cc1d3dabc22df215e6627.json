y != '*' &&\n                key != '/' &&\n                key != '+' &&\n                key != '-'\n            ) {\n                key += this.current;        \n            } else if (key === '=') {\n                equals();\n            } else if (key === 'AC') {\n                clear();\n            } else if (key === '*') {\n                multiply();\n            } else if (key === '/') {\n                divide();\n            } else if (key === '+') {\n                plus();\n            } else if (key === '-') {\n                minus();\n            }else if (key === 'Del') {\n                Del();\n            }\n\n            //Equal to\n            function equals() {\n                if((this.current).indexOf(\"^\") > -1){\n                    var base = (this.current).slice(0, (this.current).indexOf(\"^\"));\n                    var exponent = (this.current).slice((this.current).indexOf(\"^\") + 1);\n                    this.current = eval(\"Math.pow(\" + base + \",\" + exponent + \")\");\n                }\n                else{\n                    this.current = eval(this.current)\n                }\n            }\n\n            //AC(Clear)\n            function clear() {\n                this.current = \"\";\n            }\n\n            //Del\n            function Del() {\n                this.current = this.current.substring(0, this.current.length - 1);\n            }\n\n            // *\n            function multiply() {\n            this.current += \"*\";\n            }\n\n            // /\n            function divide() {\n            this.current +=  \"/\";\n            }\n\n            // +\n            function plus() {\n            this.current +=  \"+\";\n            }\n\n            // -\n            function minus() {\n            this.current +=  \"-\";\n            }\n        }\n    },\n}\n</script>\n\n<style scoped>\n.outline{\n    width: 375px;\n    height: 667px;\n    background-color: #000;\n    margin: 0 35% 0 35%;\n    b